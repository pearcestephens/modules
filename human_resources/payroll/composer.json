{
    "name": "cis/payroll",
    "description": "CIS Payroll Module with Xero Integration",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "tecnickcom/tcpdf": "^6.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "CIS\\Payroll\\": "lib/",
            "HumanResources\\Payroll\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HumanResources\\Payroll\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
