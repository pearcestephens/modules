{
    "name": "cis/modules",
    "description": "CIS Modular Application - Shared Services",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "dompdf/dompdf": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "CIS\\SharedServices\\Crawler\\": "shared/services/crawler/src/",
            "CIS\\SharedServices\\ProductIntelligence\\": "shared/services/product-intelligence/src/",
            "CIS\\SharedServices\\": "shared/services/",
            "CIS\\Base\\": "base/",
            "CIS\\DynamicPricing\\": "dynamic_pricing/",
            "CIS\\Crawlers\\": "crawlers/",
            "CIS\\StockTransferEngine\\": "stock_transfer_engine/",
            "CIS\\AIIntelligence\\": "ai_intelligence/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5"
    }
}
